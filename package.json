    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
